{
    "version": 2,
    "builds": [
      {
        "src": "src/**/*.jsx",
        "use": "@vercel/node",
        "config": {
          "maxLambdaSize": "50mb"
        }
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/$1",
        "cleanUrls": true
      }
    ]
  }
  